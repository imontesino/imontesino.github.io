@incollection{hernandez-vicenHaciaBimanipulacionInteligente2019,
  title = {Hacia La {{Bimanipulacion Inteligente}} de {{Cajas}} Con {{Robots Humanoides}}},
  author = {{Hernandez-Vicen}, J and {Garc{\'i}a-Haro}, Juan Miguel and Montesino, Ignacio and Martinez, Santiago and Balaguer, C},
  year = {2019},
  month = dec,
  pages = {28},
  abstract = {Debido al continuo incremento del comercio electr\'onico a nivel mundial, cada a\~no aumenta m\'as el volumen de mercancias y paquetes que son entregados a particulares. Todo este movimiento lleva detr\'as un conjunto de operaciones log\'isticas que generan beneficios econ\'omicos y que permiten que estos paquetes lleguen a los clientes. Sin ir m\'as lejos, en 2018 la log\'istica en Espa\~na supuso el 7,8\% del PIB. Las operaciones log\'isticas de cada pa\'is son medidas por el Banco Mundial por medio del ``Logistics Performance Index'' (\'Indice de Desempe\~no Log\'istico) y Espa\~na del 2010 al 2018 ha escalado del puesto 25 a la posici\'on 17. Para poder continuar siendo un foco de atracci\'on de empresas en este sector, es necesario optimizar estos procesos y desarrollar nuevas tecnolog\'ias que nos permitan seguir siendo foco de atracci\'on para empresas importantes. En este marco se ha pensado el desarrollo de la aplicaci\'on de bimanipulaci\'on de cajas con el robot humanoide TEO de la UC3M, perteneciente al RoboticsLab.},
  file = {/home/ignamonte/Zotero/storage/YRJZ4DVR/Hernandez-Vicen et al. - 2019 - Hacia la Bimanipulacion Inteligente de Cajas con R.pdf}
}

