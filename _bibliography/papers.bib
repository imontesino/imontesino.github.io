@inproceedings{montesinovalleImpedanceBasedGym2022,
  title = {{Impedance based Gym environment for the IIWA Collaborative robot towards human robot interaction}},
  booktitle = {{XLIII Jornadas de Autom\'atica}},
  author = {Montesino Valle, Ignacio and {\L}ukawski, Bartek and Victores, Juan G. and Jard{\'o}n Huete, Alberto and Balaguer, Carlos},
  year = {2022},
  pages = {762--769},
  publisher = {{Universidade da Coru\~na. Servizo de Publicaci\'ons}},
  urldate = {2023-01-13},
  abstract = {As rehabilitation physiotherapy is on the rise, so too is the desire for robust and adaptable robotic solutions to standarise and automate common procedures. Yet modern control techniques have still to migrate to human-interaction centered environments such as the ones required in physiotherapy. This is in great part due to the lack of learning environments that can leverage modern force controlled robots. The aim of this paper is therefore to introduce a new reinforcement learning (RL) environment for learning and training of a force controlled robotic manipulator both in simulation and in the real world. This problem can be divided into three components, each one depending on the last one. First, a robust control driver is required that bridges between the robots native programming language (C++) and the Python programming language, where most RL algorithms are implemented. Second, a cartesian impedance controller that can be fitted to work both in simulation and in the real world, abstracting away most of the complexity of rigid body dynamics. And finally the construction of the RL environment based on the ubiquitous OpenAI Gym interface.},
  copyright = {Atribuci\'on-NoComercial-CompartirIgual 4.0 Internacional (CC BY-NC-SA 4.0) https://creativecommons.org/licenses/by-nc-sa/4.0/deed.es},
  isbn = {978-84-9749-841-8},
  langid = {spanish},
  annotation = {Accepted: 2022-09-02T08:51:02Z},
  file = {/home/ignamonte/Zotero/storage/7B5GPDBC/Montesino Valle et al. - 2022 - Entorno de Gym basado en impedancia para el robot .pdf},
  selected = {true}
}
